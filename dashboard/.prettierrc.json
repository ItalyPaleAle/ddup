{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 4,
  "trailingComma": "es5",
  "printWidth": 120,
  "useTabs": false,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "overrides": [
    {
      "files": ["*.json", "*.yaml"],
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": ["*.jsx", "*.tsx"],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
